<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:width="70dp" android:height="90dp">
        <shape android:shape="rectangle">
            <solid android:color="#d7d7d7"/>
            <corners android:radius="8dp"/>
        </shape>

    </item>


    <item android:height="5dp" android:width="30dp" android:top="@dimen/margin_10dp" android:left="@dimen/margin_medium_3">
        <shape android:shape="rectangle">
            <solid android:color="@color/black"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>



    <item android:height="@dimen/margin_medium" android:width="@dimen/margin_medium" android:gravity="bottom|left" android:bottom="@dimen/margin_medium_2" android:left="-3dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>

    <item android:height="@dimen/margin_medium" android:width="@dimen/margin_medium" android:gravity="bottom|right" android:bottom="@dimen/margin_medium_2" android:right="-3dp">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
        </shape>
    </item>
</layer-list>